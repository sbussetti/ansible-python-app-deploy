server {
  # TODO: should all be templatized
  listen  83;
  access_log /var/log/nginx/circushttp-access.log;
  error_log /var/log/nginx/circushttp-error.log error;

  location ~/media/*(.jpg|.css|.js)$ {
    alias /usr/local/lib/python2.7/dist-packages/circus/web/;
  }

  location / {
      proxy_pass http://127.0.0.1:8383;
      # auth_basic            "Restricted";
      # auth_basic_user_file  /etc/nginx/htpasswd;
  }
}
